<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js" integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T" crossorigin="anonymous"></script>
</body>

<!--navbar start-->
<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <!--<a class="navbar-brand" href="/">Sweater</a>-->
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item">
                <a class="nav-link" href="/">Детектор {{detector.address}}</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/api/detector">Данные в xml</a>
            </li>
        </ul>
        <!--<div class="navbar-text mr-3">${name}</div>-->
    </div>
</nav>
<!--navbar end-->

<!--table start-->
<div class="mx-auto" style="width: 80%;">
    <table class="table table-striped my-3">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Параметр</th>
            <th scope="col">Значение</th>
            <th scope="col">Единица измерения</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <th scope="row">1</th>
            <td>ip-адрес</td>
            <td>{{detector.ip}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">2</th>
            <td>mac-адрес</td>
            <td>{{detector.mac}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">3</th>
            <td>Серийный номер</td>
            <td>{{detector.serial}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">4</th>
            <td>Дата ввода в эксплуатацию</td>
            <td>{{detector.startData}}</td>
            <td>YYYY-MM-DD HH-MM-SS</td>
        </tr>
        <tr>
            <th scope="row">5</th>
            <td>Гарантийный период</td>
            <td>{{detector.warrantyPeriod}}</td>
            <td>лет (год)</td>
        </tr>
        <tr>
            <th scope="row">6</th>
            <td>Время непрерывной работы</td>
            <td>{{detector.workTime}}</td>
            <td>час(а/ов)</td>
        </tr>
        <tr>
            <th scope="row">7</th>
            <td>Общее время работы</td>
            <td>{{detector.allWorkTime}}</td>
            <td>час(а/ов)</td>
        </tr>
        <tr>
            <th scope="row">8</th>
            <td>Количество проходов</td>
            <td>{{detector.serial}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">9</th>
            <td>Количество срабатываний</td>
            <td>{{detector.positiveDetectedCount}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">10</th>
            <td>Рабочая частота</td>
            <td>{{detector.frequency}}</td>
            <td>Гц</td>
        </tr>
        <tr>
            <th scope="row">11</th>
            <td>Ошибки</td>
            <td>{{detector.errors}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">12</th>
            <td>Адрес местоположения</td>
            <td>{{detector.address}}</td>
            <td></td>
        </tr>
        <tr>
            <th scope="row">13</th>
            <td>Время работы на аварийном питании</td>
            <td>{{detector.emergencyPowerTime}}</td>
            <td>час(а/ов)</td>
        </tr>
        </tbody>
    </table>
</div>
<!--table end-->